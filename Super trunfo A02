    char estado[20] = "Rio de Janeiro";
    char codigo_carta[5] = "A02";
    char nome_cidade[20] = "RIO DE JANEIRO";
    int populacao = 6730000;
    float area_km = 43780;
    double PIB = 1150000000.0;
    int numero_pontos_turisticos = 58;

    printf("Carta Super Trunfo\n");
    printf("Codigo: %s\n", codigo_carta);
    printf("Cidade: %s\n", nome_cidade);
    printf("Estado: %s\n", estado);
    printf("Populacao: %d habitantes\n", populacao);
    printf("Area: %.2f kmÂ²\n", area_km);
    printf("PIB: %.2lf\n", PIB);
    printf("Pontos Turisticos: %d\n", numero_pontos_turisticos);

    return 0;
}